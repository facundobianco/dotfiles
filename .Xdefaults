! Urxvt 
! -----
URxvt.termName:		rxvt-256color
URxvt.title:		urxvt
URxvt.background:	#000000
URxvt.foreground:	#BDE090
URxvt.cursorColor:      #BDE090
URxvt.internalBorder:	0
URxvt.font:		xft:dina:size=11pt:antialias=true:hinting=true
URxvt.boldFont: 	xft:dina:size=11pt:antialias=true:hinting=true:style=bold
URxvt.shading:    	100
URxvt.inheritPixmap: 	False
URxvt.cursorBlink:      False
URxvt.scrollBar:	False
URxvt.iconFile:		/home/vando/.icons/apps/terminal-icon.png
URxvt.saveLines:        65535
URxvt.print-pipe:       cat > /home/vando/`date +urxvt-dump.%s`
URxvt.perl-ext:		default,-searchable-scrollback,clipboard,url-select
URxvt.keysym.C-Insert:	perl:clipboard:copy
URxvt.keysym.S-Insert:	perl:clipboard:paste
URxvt.keysym.M-Insert:	perl:clipboard:paste_escaped
URxvt.keysym.M-s:       perl:url-select:select_next
URxvt.urlLauncher:	firefox
URxvt.underlineURLs:    False
URxvt.cutchars: 	"'()*<>[]{|}


